scrap website : https://www.hypedc.com/
fetch product info :  {"product_url": "https://www.hypedc.com/nmd-r2-pk-white-white-black.html/", "gender": "Men", "product_color": "White/White/Black", "currency": "AUD", "product_price": "269.95", "product_name": "NMD_R2 PK"} and store in database

-- Database: hypedcDB

-- DROP DATABASE "hypedcDB";

CREATE DATABASE "hypedcDB"
    WITH 
    OWNER = postgres
    ENCODING = 'UTF8'
    LC_COLLATE = 'en_US.UTF-8'
    LC_CTYPE = 'en_US.UTF-8'
    TABLESPACE = pg_default
    CONNECTION LIMIT = -1;
